package Java_fundamentals;

import java.util.Scanner;

public class Question27 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] arr=new int[n];
	    for(int i=0;i<n;i++) {
	    	arr[i]=sc.nextInt();
	    }
	    for(int i=0;i<n-1;i++) {
	    	for(int j=0;j<n;i++) {
	    		if(arr[j+1]==arr[j]) {
	    			arr[j]++;
	    		}
	    	}
	    }
	    for(int i=0;i<n;i++) {
	    	System.out.print(arr[i]);
	    }
		// TODO Auto-generated method stub

	}

}
